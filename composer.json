{
  "name": "teknasyon/suite",
  "description": ":SDK for suite platform",
  "license": "MIT",
  "authors": [
    {
      "name": "Amir",
      "email": "amirshadanfar@teknasyon.com"
    }
  ],
  "homepage": "https://github.com/teknasyon/suite",
  "keywords": [
    "Laravel",
    "Suite"
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/amir-shadanfar/suite-sdk"
    }
  ],
  "require": {
    "illuminate/support": "~7|~8",
    "illuminate/http": "8.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "orchestra/testbench": "~5|~6"
  },
  "autoload": {
    "psr-4": {
      "Teknasyon\\Suite\\": "src/"
    },
    "files": [
      "/src/Helpers/General.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Teknasyon\\Suite\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Teknasyon\\Suite\\SuiteServiceProvider"
      ],
      "aliases": {
        "Suite": "Teknasyon\\Suite\\Facades\\Suite"
      }
    }
  }
}
