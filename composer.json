{
    "name": "suite/suite",
    "description": ":SDK for suite platform",
    "license": "MIT",
    "authors": [
        {
            "name": "Amir",
            "email": "amirshadanfar@teknasyon.com"
        }
    ],
    "homepage": "git@github.com:amir-shadanfar/suite-sdk.git",
    "keywords": [
        "Laravel",
        "Suite"
    ],
    "require": {
        "illuminate/support": "~7|~8",
        "illuminate/http": "8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Suite\\Suite\\": "src/"
        },
        "files": [
            "/src/Helpers/General.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Suite\\Suite\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Suite\\Suite\\SuiteServiceProvider"
            ],
            "aliases": {
                "Suite": "Suite\\Suite\\Facades\\Suite"
            }
        }
    }
}
