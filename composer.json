{
  "name": "teknasyon/suite",
  "type": "library",
  "description": "SDK for Suite",
  "keywords": [
    "suite",
    "sdk",
    "laravel"
  ],
  "homepage": "https://github.com/amir-shadanfar/suite-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Amir Shadanfar",
      "email": "amirshadanfar@teknasyon.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "~7|~8"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "orchestra/testbench": "~5|~6"
  },
  "autoload": {
    "psr-4": {
      "Teknasyon\\Suite\\": "src"
    },
    "autoload-dev": {
      "psr-4": {
        "Teknasyon\\Suite\\Tests\\": "tests"
      }
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Teknasyon\\Suite\\SuiteServiceProvider"
      ],
      "aliases": {
        "Suite": "Teknasyon\\Suite\\Facade\\SuiteFacade"
      }
    }
  }
}